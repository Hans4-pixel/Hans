{
  "[rocket-mvbasic]": {
    "editor.codeLens": false
  },
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  "editor.tabSize": 2,
  "files.associations": {
    "*.embeddedhtml": "html",
    "*.ejs": "html",
    "**/src/main/resources/deploy.json": "json",
    "**/src/main/resources/deploy_ch2.json": "json",
    "*.jsonc": "json",
    "app/*.html": "ejs",
    "**/.rmv/config/*.json": "jsonc"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.ruby-lsp": true,
    "**/.rmv/catalog/**": true,
    "*/_*": false
  },
  "files.trimTrailingWhitespace": true,
  "javascript.preferences.importModuleSpecifier": "relative",
  "json.schemas": [
    {
      "fileMatch": ["app/manifest/*/*.json"],
      "url": "https://json.schemastore.org/chrome-manifest"
    },
    {
      // apply our package.json schema to our our root `package.json` only
      "fileMatch": ["package.json", "!node_modules/**/package.json"],
      "url": "./.vscode/package.json-schema.json"
    }
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "ocaml.sandbox": {
    "kind": "global"
  }
}
